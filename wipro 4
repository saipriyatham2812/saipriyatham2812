import java.util.*;
public class Main
{
public static void main(String[] args) {
Scanner in = new Scanner(System.in);
System.out.print("Enter the number: ");
int number = in.nextInt();
int fact=1;
for(int i=1;i<=number;i++){
fact=fact*i;
}
System.out.println("The Factorial of "+number+" is: "+fact);


}
}

